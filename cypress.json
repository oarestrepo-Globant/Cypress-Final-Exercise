{  
  "baseUrl": "https://www.demoblaze.com/",
  "env": {
    "TAGS": "not @ignore"
  },
  "testFiles": "**/*.feature",
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reporterDir": "cypress/reports/mocha",
      "quite": true,
      "overwrite": false,
      "html": false,
      "json": true
    }
  }
}
